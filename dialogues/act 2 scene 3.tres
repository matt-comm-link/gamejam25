"condition": {},
"link": &"1_74",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 19
},
"1_74": {
"dialogue": "It’s kind of a local–huh?",
"offset": Vector2(16140, 420),
"options": {
