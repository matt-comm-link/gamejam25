"options": {
0: {
"condition": {},
"link": &"1_125",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 3
},
"1_125": {
"dialogue": "(Urk…)",
"offset": Vector2(18460, 220),
"options": {
0: {
"condition": {},
"link": &"1_126",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 21
},
"1_126": {
"dialogue": "(The things I do for a stable timeline…)",
"offset": Vector2(18800, 220),
"options": {
0: {
"condition": {},
"link": &"1_127",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 21
},
"1_127": {
"dialogue": "(But we can’t waste time. They need to get moving. I’ll have to give them a reason.)",
"offset": Vector2(19180, 460),
"options": {
0: {
"condition": {},
"link": &"1_128",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 18
},
"1_128": {
"dialogue": "Hey! Come back here!",
"offset": Vector2(19595.9, 559.782),
"options": {
0: {
"condition": {},
"link": &"1_129",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 3
},
"1_129": {
