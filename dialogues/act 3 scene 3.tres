"options": {
0: {
"condition": {},
"link": &"1_121",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 3
},
"1_121": {
"dialogue": "No… it just grabbed Larissa’s drawing from my hands.",
"offset": Vector2(17060, 220),
"options": {
0: {
"condition": {},
"link": &"1_122",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 9
},
"1_122": {
