"offset": Vector2(4100, -240),
"options": {
0: {
"condition": {},
"link": &"1_15",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 15
},
"1_15": {
"dialogue": "What.",
"offset": Vector2(4440, -220),
"options": {
0: {
"condition": {},
"link": &"1_16",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 8
},
"1_16": {
"dialogue": " I’ve been watching it. I noticed the light was getting stronger the further we got into the woods… but then it started growing weaker. Up until we made camp. It’s been at the same intensity ever since we stopped moving.",
"offset": Vector2(4780, -220),
"options": {
0: {
"condition": {},
"link": &"1_17",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 16
},
"1_17": {
