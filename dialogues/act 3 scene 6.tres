"options": {
0: {
"condition": {},
"link": &"1_26",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 3
},
"1_26": {
"dialogue": "(Marl just charged it with their pocket knife! That is… not going to work. But if they can just grab its attention…)",
"offset": Vector2(9360, 220),
"options": {
0: {
"condition": {},
"link": &"3_5",
"text": ""
}
},
"size": Vector2(300, 240),
"speaker": 18
},
"1_27": {
"dialogue": "Grah!",
